{
  "author": "tony_quetano@planttheidea.com",
  "browser": "dist/curriable.js",
  "browserslist": [
    "defaults",
    "Explorer >= 9",
    "Safari >= 6",
    "Opera >= 15",
    "iOS >= 8",
    "Android >= 4"
  ],
  "bugs": {
    "url": "https://github.com/planttheidea/curriable/issues"
  },
  "description": "Convert any method to be curriable with placeholder support",
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "benchee": "^1.0.3",
    "cli-table": "^0.3.1",
    "cli-table2": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "in-publish": "^2.0.0",
    "jest": "^23.6.0",
    "lodash": "^4.17.10",
    "ora": "^3.0.0",
    "performance-now": "^2.1.0",
    "ramda": "^0.26.1",
    "rollup": "^1.1.1",
    "rollup-plugin-terser": "^4.0.2",
    "rollup-plugin-typescript2": "^0.19.2",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.4"
  },
  "keywords": [
    "curry",
    "functional"
  ],
  "license": "MIT",
  "homepage": "https://github.com/planttheidea/curriable#readme",
  "main": "dist/curriable.cjs.js",
  "module": "dist/curriable.esm.js",
  "name": "curriable",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/curriable.git"
  },
  "scripts": {
    "benchmark": "rimraf dist && npm run build && node ./benchmarks/index.js",
    "build": "NODE_ENV=production rollup -c",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development webpack-dev-server --progress --colors --config=webpack/webpack.config.js",
    "lint": "NODE_ENV=test tslint 'src/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prepublish": "if in-publish; then npm run prepublish:compile; fi",
    "prepublish:compile": "npm run lint && npm run test:coverage && npm run clean && npm run build",
    "start": "npm run dev",
    "test": "NODE_PATH=. jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "sideEffects": false,
  "types": "index.d.ts",
  "version": "1.2.4"
}
