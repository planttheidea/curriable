{
  "author": "tony_quetano@planttheidea.com",
  "ava": {
    "babel": "inherit",
    "failFast": true,
    "files": ["test/*.js"],
    "require": ["babel-register"],
    "source": ["src/*.js"],
    "verbose": true
  },
  "browserslist": ["defaults", "Explorer >= 9", "Safari >= 6", "Opera >= 15", "iOS >= 8", "Android >= 4"],
  "bugs": {
    "url": "https://github.com/planttheidea/curriable/issues"
  },
  "description": "A tiny, fast method to convert any method to be curriable",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "benchmark": "^2.1.4",
    "cli-table": "^0.3.1",
    "cli-table2": "^0.2.0",
    "eslint": "^4.18.2",
    "eslint-config-rapid7": "^2.11.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "html-webpack-plugin": "^2.30.1",
    "in-publish": "^2.0.0",
    "lodash": "^4.17.5",
    "nyc": "^11.4.1",
    "optimize-js-plugin": "^0.0.4",
    "ora": "^2.0.0",
    "performance-now": "^2.1.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rollup": "^0.57.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-uglify": "^3.0.0",
    "sinon": "^4.4.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "keywords": ["curry", "functional"],
  "license": "MIT",
  "homepage": "https://github.com/planttheidea/curriable#readme",
  "main": "lib/index.js",
  "module": "es/index.js",
  "name": "curriable",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/curriable.git"
  },
  "scripts": {
    "benchmark": "npm run transpile:lib && NODE_ENV=production node ./benchmarks/index.js",
    "build": "NODE_ENV=production rollup -c",
    "clean": "npm run clean:lib && npm run clean:es && npm run clean:dist",
    "clean:dist": "rimraf dist",
    "clean:lib": "rimraf lib",
    "clean:es": "rimraf es",
    "dev": "NODE_ENV=development webpack-dev-server --colors --progress --config=webpack/webpack.config.dev.js",
    "dist": "npm run clean:dist && npm run build",
    "lint": "NODE_ENV=test eslint src",
    "lint:fix": "NODE_ENV=test eslint src --fix",
    "prepublish": "if in-publish; then npm run prepublish:compile; fi",
    "prepublish:compile":
      "npm run lint && npm run test:coverage && npm run transpile:lib && npm run transpile:es && npm run dist",
    "start": "npm run dev",
    "test": "NODE_PATH=. NODE_ENV=production BABEL_ENV=test ava",
    "test:coverage": "nyc npm test",
    "test:watch": "npm test -- --watch",
    "transpile:lib": "npm run clean:lib && BABEL_ENV=lib babel src --out-dir lib",
    "transpile:es": "npm run clean:es && BABEL_ENV=es babel src --out-dir es"
  },
  "version": "1.0.0"
}
